{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Speeder": {
    "MeasurementIntervalSeconds": 900,
    "UseOokla": true,
    "UseIperf": true
  },
  "Iperf": {
    "Servers": [
      "iperf.worldstream.nl#5201",
      "ping.online.net#5200-5209"
    ],
    "ExePath": "/usr/bin/speedtest"
  },
  "Ookla": {
    "ExePath": "/usr/bin/speedtest",
    "ServerId": null
  }
}
